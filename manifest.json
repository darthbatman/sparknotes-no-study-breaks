{
"name":"sparknotesTest",
"description":"Hides distracting \"Take a study break!\" and \"From the B&N Book Blog\"",
"version":"1",
"manifest_version":2,

"browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Sparknotes Test"
  },

"content_scripts": [
    {
      "matches": ["http://www.sparknotes.com/lit/*","https://www.sparknotes.com/lit/*"],
      "js": ["hide.js"],
      "run_at": "document_end"
    }
  ]
}